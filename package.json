  "scripts": {
    "admin-dev": "webpack serve --hot --open 'Google Chrome' --mode development --env APP_CONFIG=./app.admin.config.js",
    "admin-build": "webpack build --mode production --env APP_CONFIG=./app.admin.config.js",
    "admin-serve": "http-server 'dist' -a 'localhost' -o '/admin' -p 8011 --cros --proxy http://localhost:8011/admin?",
