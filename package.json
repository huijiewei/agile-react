{
  "name": "agile-react",
  "version": "1.0.0",
  "description": "Agile React Project",
  "private": true,
  "keywords": [
    "react",
    "typescript",
    "webpack 5",
    "admin boilerplate"
  ],
  "scripts": {
    "admin-dev": "webpack serve --hot --mode development --env APP_CONFIG=$PWD/app.admin.config.js",
